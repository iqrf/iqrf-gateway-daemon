{
    "$schema": "http://apidocs.iqrfsdk.org/iqrfgd2-schemas/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.iqrftech.self-desc",
        "name": "schema__iqrf__IqrfDpa",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "type": "object",
    "properties": {
        "component": {
            "type": "string",
            "description": "Name of component.",
            "enum": [
                "iqrf::IqrfDpa"
            ]
        },
        "instance": {
            "type": "string",
            "description": "Recomended iqrf::IqrfDpa-(id)",
            "default": "iqrf::IqrfDpa-1"
        },
        "DpaHandlerTimeout": {
            "type": "integer",
            "description": "...",
            "default": 500
        },
        "CommunicationMode": {
            "type": "string",
            "description": "...",
            "default": "STD"
        },
        "BondedNodes": {
            "type": "integer",
            "description": "...",
            "default": 10
        },
        "DiscoveredNodes": {
            "type": "integer",
            "description": "...",
            "default": 10
        },
        "ResponseTime": {
            "type": "string",
            "description": "...",
            "enum": [
                "k40Ms",
                "k360Ms",
                "k680Ms",
                "k1320Ms",
                "k2600Ms",
                "k5160Ms",
                "k10280Ms",
                "k20620Ms"
            ]
        },
        "RequiredInterfaces": {
            "type": "array",
            "description": "Array of required interfaces.",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "default": "iqrf::IMessagingService",
                        "description": "..."
                    },
                    "target": {
                        "type": "object"
                    }
                },
                "required": [
                    "name",
                    "target"
                ]
            }
        }
    },
    "required": [
        "component",
        "instance"
    ]
