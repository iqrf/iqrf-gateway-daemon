{
    "$schema": "http://apidocs.iqrfsdk.org/iqrfgd2-schemas/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.iqrftech.self-desc",
        "name": "shape__TraceFileService-IqrfGwDaemon",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "type": "object",
    "properties": {
        "component": {
            "type": "string",
            "description": "...",
            "default": "shape::TraceFileService"
        },
        "instance": {
            "type": "string",
            "description": "...",
            "default": "shape::TraceFileService-Cobalt"
        },
        "path": {
            "type": "string",
            "description": "...",
            "default": ""
        },
        "filename": {
            "type": "string",
            "description": "...",
            "default": "IqrfDaemon.txt"
        },
        "VerbosityLevels": {
            "type": "array",
