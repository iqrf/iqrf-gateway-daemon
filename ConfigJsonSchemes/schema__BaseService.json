                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "default": "iqrf::IMessagingService",
                        "description": "..."
                    },
                    "target": {
                        "type": "object",
                        "properties":{                            
                            "instance":{
                                "type":"string",
                                "description": "...",
                                "default":"MqMessaging"
                            }
                        }
                    }
                },
                "required": [
                    "channel",
                    "level"
                ]
            }
        }        
    },
    "required": [
        "component",
        "instance"
    ]
