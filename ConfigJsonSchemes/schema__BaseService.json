{
    "$schema": "http://apidocs.iqrfsdk.org/iqrfgd2-schemas/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.iqrftech.self-desc",
        "name": "iqrf__BaseService",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "type": "object",
    "properties": {
        "component": {
            "type": "string",
            "description": "...",
            "default": "iqrf::BaseService"
        },
        "instance": {
            "type": "string",
            "description": "...",
            "default": "BaseServiceForMQ"
        },
        "RequiredInterfaces": {
            "type": "array",
            "description": "Array of verbosity levels.",
