all: build-all push-all

build-all: build-stretch build-buster

build-buster:
	$(MAKE) -C buster/ build-all

build-stretch:
	$(MAKE) -C stretch/ build-all

push-all: push-stretch push-buster

push-buster:
	$(MAKE) -C buster/ push-all

push-stretch:
	$(MAKE) -C stretch/ push-all
