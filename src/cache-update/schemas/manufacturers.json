{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "manufacturers.json",
	"title": "Manufacturers schema",
	"description": "IQRF Repository manufacturers data schema.",
	"type": "array",
	"additionalItems": false,
	"items": {
		"title": "Manufacturer item",
		"type": "object",
		"required": [
      "manufacturerID",
			"companyID",
			"name"
		],
		"additionalProperties": false,
		"properties": {
      "manufacturerID": {
        "title": "Manufacturer ID",
        "type": "integer",
        "minimum": 0
      },
			"companyID": {
				"title": "Company ID",
				"type": "integer",
				"minimum": 0
			},
			"name": {
				"title": "Manufacturer name",
				"type": "string"
			}
		}
	}
}