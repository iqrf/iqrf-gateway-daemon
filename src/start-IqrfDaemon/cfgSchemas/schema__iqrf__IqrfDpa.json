        "component": {
            "type": "string",
            "description": "Name of component.",
            "enum": [
                "iqrf::IqrfDpa"
            ]
        },
        "instance": {
            "type": "string",
            "description": "Recomended iqrf::IqrfDpa-(id)",
            "default": "iqrf::IqrfDpa-1"
        },
        "DpaHandlerTimeout": {
            "type": "integer",
            "description": "...",
            "default": 500
        },
        "interfaceCheckPeriod": {
            "title": "Interface check period",
            "description": "Interface readiness check period",
            "type": "integer",
            "minimum": 0,
            "default": 30
        },
        "RequiredInterfaces": {
            "type": "array",
            "description": "Array of required interfaces.",
            "items": {
                "type": "object",
                "required": [
                    "name",
                    "target"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "default": "iqrf::IMessagingService",
                        "description": "..."
                    },
                    "target": {
                        "type": "object"
                    }
                }
            }
        }
    }
}
