{
    "$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.iqrftech.self-desc",
        "name": "schema__iqrf__JsCache",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "type": "object",
    "properties": {
        "component": {
            "type": "string",
            "description": "Name of component.",
            "enum": [
                "iqrf::JsCache"
            ]
        },
        "instance": {
            "type": "string",
            "description": "Recommended iqrf::JsCache-(id)",
            "default": "iqrf::JsCache-1",
            "minLength": 1
        },
        "iqrfRepoCache": {
            "type": "string",
            "description": "...",
            "default": "iqrfRepoCache"
        },
		"urlRepo": {
            "type": "string",
            "description": "IQRF Repository URL",
            "default": "https://repository.iqrfalliance.org/api"
        },
        "checkPeriodInMinutes": {
            "type": "number",
            "description": "Check period in minutes",
            "default": 0
        },
        "RequiredInterfaces": {
            "type": "array",
            "description": "Array of required interfaces.",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "default": "iqrf::IMessagingService",
                        "description": "..."
                    },
                    "target": {
                        "type": "object"
                    }
                },
                "required": [
                    "name",
                    "target"
                ]
            }
        }
    },
    "required": [
        "component",
        "instance"
    ]
}
