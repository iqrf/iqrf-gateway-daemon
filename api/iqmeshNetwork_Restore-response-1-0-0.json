{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "IQMESH Network - Restore",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "IQMESH Network management - Restore response.",
			"const": "iqmeshNetwork_Restore"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"rsp",
				"status"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response data",
					"description": "Response body object",
					"type": "object",
					"required": [
						"progress",
						"devices"
					],
					"additionalProperties": false,
					"properties": {
						"progress": {"$ref": "file://definitions.json#/iqmesh/general/progress"},
						"devices": {
							"$id": "#/properties/data/properties/rsp/properties/devices",
							"title": "Restore information",
							"description": "Array of device(s) restoration information.",
							"type": "array",
							"minItems": 1,
							"maxItems": 240,
							"additionalItems": false,
							"items": {
								"$id": "#/properties/data/properties/rsp/properties/devices/items",
								"title": "Device restore information",
								"description": "Restore information of a single device.",
								"type": "object",
								"required": [
									"deviceAddr",
									"restoreStatus"
								],
								"additionalProperties": false,
								"properties": {
									"deviceAddr": {
										"$ref": "file://definitions.json#/embed/coordinator/reqAddr",
										"description": "Address of device in network. 0 = Coordinator, 1..239 = Node."
									},
									"restoreStatus": {
										"$id": "#/properties/data/properties/rsp/properties/devices/items/properties/restoreStatus",
										"title": "Restore status",
										"description": "Result of restore operation.",
										"type": "boolean"
									}
								}
							}
						}
					}
				},
				"raw": {"$ref": "file://definitions.json#/general/response/raw"},
				"insId": {"$ref": "file://definitions.json#/general/response/insId"},
				"status": {"$ref": "file://definitions.json#/general/response/status"},
				"statusStr": {"$ref": "file://definitions.json#/general/response/statusStr"}
			}
		}
	}
}
