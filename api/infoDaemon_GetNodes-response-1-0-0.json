{
  "$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
  "self": {
    "vendor": "com.iqrftech.self-desc",
    "name": "infoDaemon_GetNodes-response",
    "format": "jsonschema",
    "version": "1-0-0"
  },
  "type": "object",
  "required": [
    "mType",
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "mType": {
      "type": "string",
      "description": "Daemon information - get all bonded nodes.",
      "enum": [
        "infoDaemon_GetNodes"
      ]
    },
    "data": {
      "type": "object",
      "required": [
        "msgId",
        "rsp",
        "status"
      ],
      "additionalProperties": false,
      "properties": {
        "msgId": {
          "type": "string",
          "description": "Message identification for binding request with response."
        },
        "rsp": {
          "type": "object",
          "description": "Response data",
          "required": [
            "nodes"
          ],
          "additionalProperties": false,
          "properties": {
            "nodes": {
              "type": "array",
              "description": "Nodes data",
              "items": {
                "type": "object",
                "description": "Node data",
                "required": [
                  "nAdr",
                  "mid",
                  "disc",
                  "hwpid",
                  "hwpidVer",
                  "osBuild",
                  "dpaVer"
                ],
                "additionalProperties": false,
                "properties": {
                  "nAdr": {
                    "title": "Node address",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 255
                  },
                  "mid": {
                    "title": "Module ID",
                    "type": "integer",
                    "minimum": 0
                  },
                  "disc": {
                    "title": "Discovery status",
                    "type": "boolean"
                  },
                  "hwpid": {
                    "title": "Hardware profile ID",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 65535
                  },
                  "hwpidVer": {
                    "title": "Hardware profile ID version",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 65535
                  },
                  "osBuild": {
                    "title": "OS build",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 65535
                  },
                  "dpaVer": {
                    "title": "DPA version",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 65535
                  },
                  "metaData": {
                    "title": "User metadata",
                    "oneOf": [
                      {
                        "type": "object"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "insId": {
          "type": "string",
          "description": "IQRF GW daemon instance identification."
        },
        "status": {
          "type": "integer",
          "description": "Status of operation."
        },
        "statusStr": {
          "type": "string",
          "description": "IQRF GW daemon API (general or mType) status in string form."
        }
      }
    }
  }
}
