{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Embed FRC - SendSelective",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "Embedded peripheral FRC - Send Selective response.",
			"const": "iqrfEmbedFrc_SendSelective"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"rsp",
				"status"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response data",
					"description": "Response body object",
					"type": "object",
					"required": [
						"nAdr",
						"hwpId",
						"rCode",
						"dpaVal"
					],
					"additionalProperties": false,
					"properties": {
						"nAdr": {"$ref": "file://definitions.json#/embed/coordinator/nAdr"},
						"hwpId": {"$ref": "file://definitions.json#/general/response/hwpId"},
						"rCode": {"$ref": "file://definitions.json#/general/response/rCode"},
						"dpaVal": {"$ref": "file://definitions.json#/general/response/dpaVal"},
						"result": {
							"$id": "#/properties/data/properties/rsp/properties/result",
							"title": "Result object",
							"description": "Request result data.",
							"type": "object",
							"required": [
								"status",
								"frcData"
							],
							"additionalProperties": false,
							"properties": {
								"status": {"$ref": "file://definitions.json#/embed/frc/status"},
								"frcData": {
									"$id": "#/properties/data/properties/rsp/properties/result/properties/frcData",
									"title": "FRC data.",
									"description": "Data collected from nodes. Currently only able to transfer up to 55 bytes. The remaining bytes can be retrieved useing the ExtraResult request.",
									"maxItems": 55,
									"additionalItems": false,
									"items": {"$ref": "file://definitions.json#/embed/frc/frcByte"}
								}
							}
						},
						"errorStr": {"$ref": "file://definitions.json#/general/response/errorStr"}
					}
				},
				"raw": {"$ref": "file://definitions.json#/general/response/raw"},
				"insId": {"$ref": "file://definitions.json#/general/response/insId"},
				"status": {"$ref": "file://definitions.json#/general/response/status"},
				"statusStr": {"$ref": "file://definitions.json#/general/response/statusStr"},
				"timeout": {"$ref": "file://definitions.json#/general/timeout"}
			}
		}
	}
}
