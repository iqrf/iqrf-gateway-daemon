{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "IQMESH Network - FRC Params response",
  "description": "(c) IQRF Tech",
  "type": "object",
  "required": [
    "mType",
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "mType": {
      "allOf": [
        {
          "$ref": "definitions.json#/message/type"
        },
        {
          "const": "iqmeshNetwork_FrcParams"
        }
      ]
    },
    "data": {
      "title": "Message data",
      "description": "Message metadata and body",
      "type": "object",
      "required": [
        "msgId",
        "rsp",
        "status"
      ],
      "additionalProperties": false,
      "properties": {
        "msgId": {
          "$ref": "definitions.json#/message/id"
        },
        "rsp": {
          "title": "Response",
          "description": "Holds response data",
          "type": "object",
          "required": [
            "action"
          ],
          "additionalProperties": false,
          "properties": {
            "action": {
              "$ref": "definitions.json#/iqmesh/frcParams/action"
            },
            "responseTime": {
              "$ref": "definitions.json#/iqmesh/frcParams/responseTime"
            },
            "offlineFrc": {
              "$ref": "definitions.json#/iqmesh/frcParams/offlineFrc"
            }
          }
        },
        "raw": {
          "$ref": "definitions.json#/response/raw"
        },
        "insId": {
          "$ref": "definitions.json#/response/insId"
        },
        "status": {
          "$ref": "definitions.json#/response/status"
        },
        "statusStr": {
          "$ref": "definitions.json#/response/statusStr"
        }
      }
    }
  }
}
