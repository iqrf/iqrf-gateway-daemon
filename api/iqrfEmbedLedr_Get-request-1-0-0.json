{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "Embed LEDR - Get request schema",
  "type": "object",
  "required": [
    "mType",
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "mType": {
      "$ref": "definitions.json#/general/mType",
      "const": "iqrfEmbedLedr_Get"
    },
    "data": {
      "title": "Message data",
      "description": "Message metadata and body.",
      "type": "object",
      "required": [
        "msgId",
        "req"
      ],
      "additionalProperties": false,
      "properties": {
        "msgId": {
          "$ref": "definitions.json#/general/msgId"
        },
        "req": {
          "title": "Request data",
          "description": "Request body object",
          "type": "object",
          "required": [
            "nAdr"
          ],
          "additionalProperties": false,
          "properties": {
            "nAdr": {
              "$ref": "definitions.json#/general/nAdr"
            },
            "hwpId": {
              "$ref": "definitions.json#/general/request/hwpId"
            }
          }
        },
        "timeout": {
          "$ref": "definitions.json#/general/timeout"
        },
        "returnVerbose": {
          "$ref": "definitions.json#/general/returnVerbose"
        }
      }
    }
  }
}
