{
  "$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
  "title": "IQRF Sensor Data - Worker status",
  "type": "object",
  "required": [
    "mType",
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "mType": {
      "$id": "#/properties/mType",
      "title": "Message type",
      "description": "IQRF Sensor Data - Status response.",
      "type": "string",
      "enum": [
        "iqrfSensorData_Status"
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "title": "Message data",
      "description": "Message metadata and body.",
      "type": "object",
      "required": [
        "msgId",
        "status"
      ],
      "additionalProperties": false,
      "properties": {
        "msgId": {
          "$id": "#/properties/data/properties/msgId",
          "title": "Message ID",
          "description": "Message ID used for pairing requests and responses.",
          "type": "string"
        },
        "rsp": {
          "$id": "#/properties/data/properties/rsp",
          "title": "Response object",
          "description": "Response object.",
          "type": "object",
          "required": [
            "running",
            "reading"
          ],
          "additionalProperties": false,
          "properties": {
            "running": {
              "$id": "#/properties/data/properties/rsp/properties/running",
              "title": "Worker running",
              "description": "Worker is running, and either collecting data or waiting until next reading time.",
              "type": "boolean"
            },
            "reading": {
              "$id": "#/properties/data/properties/rsp/properties/reading",
              "title": "Reading data",
              "description": "Worker is collecting data from network.",
              "type": "boolean"
            }
          }
        },
        "insId": {
          "$id": "#/properties/data/properties/insId",
          "title": "Instance ID",
          "description": "Daemon insteance ID.",
          "type": "string"
        },
        "status": {
          "$id": "#/properties/data/properties/status",
          "title": "Status code",
          "description": "Status code.",
          "type": "integer"
        },
        "statusStr": {
          "$id": "#/properties/data/properties/statusStr",
          "title": "Status string",
          "description": "Verbose status string",
          "type": "string"
        }
      }
    }
  }
}
