{
	"$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
	"title": "GetLights response",
	"description": "Retrieves addresses of devices implementing the Light standard.",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$id": "#/properties/mType",
			"title": "Message type",
			"description": "IQRF DB - GetLights response.",
			"type": "string",
			"enum": ["iqrfDb_GetLights"]
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Response data",
			"description": "Response data.",
			"type": "object",
			"required": [
				"msgId",
				"rsp"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {
					"$id": "#/properties/data/properties/msgId",
					"title": "Message ID",
					"description": "Message ID used for pairing requests and responses.",
					"type": "string"
				},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response parameters",
					"description": "Response parameters.",
					"type": "object",
					"required": ["lightDevices"],
					"additionalProperties": false,
					"properties": {
						"lightDevices": {
							"$id": "#/properties/data/properties/rsp/properties/lightDevices",
							"title": "Light devices",
							"description": "Array containing addresses of devices implementing Light standard.",
							"type": "array",
							"additionalItems": false,
							"items": {
								"$id": "#/properties/data/properties/rsp/properties/lightDevices/items",
								"title": "Device address",
								"description": "Address of device implementing Light standard.",
								"type": "integer",
								"minimum": 0,
								"maximum": 239
							}
						}
					}
				},
				"insId": {
					"$id": "#/properties/data/properties/insId",
					"title": "Instance ID",
					"description": "Daemon insteance ID.",
					"type": "string"
				},
				"status": {
					"$id": "#/properties/data/properties/status",
					"title": "Status code",
					"description": "Status code.",
					"type": "integer"
				},
				"statusStr": {
					"$id": "#/properties/data/properties/statusStr",
					"title": "Status string",
					"description": "Verbose status string",
					"type": "string"
				}
			}
		}
	}
}
