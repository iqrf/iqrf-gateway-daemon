{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "IQRF BinaryOutput - SetOutput",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "Standard peripheral Binary Output - Set Output response.",
			"const": "iqrfBinaryoutput_SetOutput"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"rsp",
				"status"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response data",
					"description": "Response body object",
					"type": "object",
					"required": [
						"nAdr",
						"hwpId",
						"rCode",
						"dpaVal"
					],
					"additionalProperties": false,
					"properties": {
						"nAdr": {"$ref": "file://definitions.json#/general/nAdr"},
						"hwpId": {"$ref": "file://definitions.json#/general/hwpId"},
						"rCode": {"$ref": "file://definitions.json#/general/response/rCode"},
						"dpaVal": {"$ref": "file://definitions.json#/general/response/dpaVal"},
						"result": {
							"$id": "#/properties/data/properties/rsp/properties/result",
							"title": "Result object",
							"description": "Request result data.",
							"type": "object",
							"required": [
								"prevVals"
							],
							"additionalProperties": false,
							"properties": {
								"prevVals": {
									"$id": "#/properties/data/properties/rsp/properties/result/properties/prevVals",
									"title": "Previous states",
									"description": "Array of boolean true/false values specifying the previous state of each binary output. false is returned for unimplemented binary outputs.",
									"type": "array",
									"additionalItems": false,
									"items": {
										"$id": "#/properties/data/properties/rsp/properties/result/properties/prevVals/items",
										"title": "Binary output state",
										"description": "Previous state of a binary output.",
										"type": "boolean"
									}
								}
							}
						},
						"errorStr": {"$ref": "file://definitions.json#/general/response/errorStr"}
					}
				},
				"raw": {"$ref": "file://definitions.json#/general/response/raw"},
				"insId": {"$ref": "file://definitions.json#/general/response/insId"},
				"status": {"$ref": "file://definitions.json#/general/response/status"},
				"statusStr": {"$ref": "file://definitions.json#/general/response/statusStr"},
				"timeout": {"$ref": "file://definitions.json#/general/timeout"}
			}
		}
	}
}
