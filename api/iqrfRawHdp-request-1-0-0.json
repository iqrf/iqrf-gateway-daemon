{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Raw Hdp message request",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "Message type for Raw Hdp communication.",
			"const": "iqrfRawHdp"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"req"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"req": {
					"$id": "#/properties/data/properties/req",
					"title": "Request data",
					"description": "Request body object",
					"type": "object",
					"required": [
						"nAdr",
						"pNum",
						"pCmd"
					],
					"additionalProperties": false,
					"properties": {
						"nAdr": {"$ref": "file://definitions.json#/general/nAdr"},
						"pNum": {"$ref": "file://definitions.json#/general/pNum"},
						"pCmd": {"$ref": "file://definitions.json#/general/request/pCmd"},
						"hwpId": {"$ref": "file://definitions.json#/general/hwpId"},
						"pData": {"$ref": "file://definitions.json#/general/request/pData"}
					}
				},
				"timeout": {"$ref": "file://definitions.json#/general/timeout"},
				"returnVerbose": {"$ref": "file://definitions.json#/general/request/returnVerbose"}
			}
		}
	}
}
