{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Daemon management - Exit",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "Daemon runtime management - exit request message.",
			"const": "mngDaemon_Exit"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"rsp",
				"status"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response data",
					"description": "Response body object",
					"type": "object",
					"required": [
						"timeToExit"
					],
					"additionalProperties": false,
					"properties": {
						"timeToExit": {
							"$id": "#/properties/data/properties/rsp/properties/timeToExit",
							"title": "Time to exit",
							"description": "Time to exit the daemon in ms.",
							"type": "number",
							"minimum": 0,
							"example": 10000
						}
					}
				},
				"insId": {"$ref": "file://definitions.json#/general/response/insId"},
				"status": {"$ref": "file://definitions.json#/general/response/status"},
				"statusStr": {"$ref": "file://definitions.json#/general/response/statusStr"}
			}
		}
	}
}
