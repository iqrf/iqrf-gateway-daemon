{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "Peripheral Device exploration - Enumerate response",
  "description": "(c) IQRF Tech",
  "type": "object",
  "required": [
    "mType",
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "mType": {
      "allOf": [
        { "$ref": "definitions.json#/general/mType" },
        { "const": "iqrfEmbedExplore_Enumerate" }
      ]
    },
    "data": {
      "title": "Message data",
      "description": "Message metadata and body",
      "type": "object",
      "required": [
        "msgId",
        "rsp",
        "status"
      ],
      "additionalProperties": false,
      "properties": {
        "msgId": {
          "$ref": "definitions.json#/general/msgId"
        },
        "rsp": {
          "title": "Response",
          "description": "Holds response data",
          "type": "object",
          "required": [
            "nAdr",
            "pNum",
            "pCmd",
            "hwpId",
            "rCode",
            "dpaVal"
          ],
          "additionalProperties": false,
          "properties": {
            "nAdr": {
              "$ref": "definitions.json#/general/nAdr"
            },
            "pNum": {
              "$ref": "definitions.json#/general/response/pNum"
            },
            "pCmd": {
              "$ref": "definitions.json#/general/response/pCmd"
            },
            "hwpId": {
              "$ref": "definitions.json#/general/response/hwpId"
            },
            "rCode": {
              "$ref": "definitions.json#/general/response/rCode"
            },
            "dpaVal": {
              "$ref": "definitions.json#/general/response/dpaVal"
            },
            "result": {
              "title": "Result",
              "description": "Contains the results of the request",
              "type": "object",
              "required": [
                "dpaVer",
                "userPerNr",
                "embeddedPers",
                "hwpid",
                "hwpidVer",
                "flags",
                "userPer"
              ],
              "additionalProperties": false,
              "properties": {
                "dpaVer": {
                  "$ref": "definitions.json#/explore/dpaVer"
                },
                "userPerNr": {
                  "$ref": "definitions.json#/explore/userPerNr"
                },
                "embeddedPers": {
                  "$ref": "definitions.json#/explore/embeddedPers"
                },
                "hwpid": {
                  "$ref": "definitions.json#/general/response/hwpId"
                },
                "hwpidVer": {
                  "$ref": "definitions.json#/explore/hwpIdVersion"
                },
                "flags": {
                  "$ref": "definitions.json#/explore/flags"
                },
                "userPer": {
                  "$ref": "definitions.json#/explore/userPeripherals"
                }
              }
            },
            "errorStr": {
              "$ref": "definitions.json#/general/response/errorStr"
            }
          }
        },
        "raw": {
          "$ref": "definitions.json#/general/response/raw"
        },
        "insId": {
          "$ref": "definitions.json#/general/response/insId"
        },
        "status": {
          "$ref": "definitions.json#/general/response/status"
        },
        "statusStr": {
          "$ref": "definitions.json#/general/response/statusStr"
        },
        "timeout": {
          "$ref": "definitions.json#/general/timeout"
        }
      }
    }
  }
}
