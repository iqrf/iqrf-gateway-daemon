{
	"$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
	"self": {
		"vendor": "com.iqrftech.self-desc",
		"name": "mngService_TrData-response",
		"format": "jsonschema",
		"version": "1-0-0"
	},
	"title": "Daemon Management - Write to transceiver",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$id": "#/properties/mType",
			"title": "Message type",
			"description": "Daemon management - Write to transceiver response.",
			"type": "string",
			"enum": ["mngService_TrData"]
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body.",
			"type": "object",
			"required": [
				"msgId",
        "rsp"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {
					"$id": "#/properties/data/properties/msgId",
					"title": "Message ID",
					"description": "Message ID used for pairing requests and responses.",
					"type": "string"
				},
        "rsp": {
          "$id": "#/properties/data/properties/rsp",
          "title": "Response data",
          "description": "Object containing command response data.",
          "type": "object",
          "required": [
            "packet"
          ],
          "additionalProperties": false,
          "properties": {
            "packet": {
              "$id": "#/properties/data/properties/req/properties/packet",
              "title": "DPA confirmation or response",
              "description": "DPA protocol confirmation or response packet.",
              "type": "array",
              "minItems": 8,
              "maxItems": 64,
              "items": {
                "title": "Packet data",
                "description": "Individual packet byte.",
                "type": "integer",
                "minimum": 0,
                "maximum": 255
              }
            }
          }
        },
				"insId": {
					"$id": "#/properties/data/properties/insId",
					"title": "Instance ID",
					"description": "Daemon insteance ID.",
					"type": "string"
				},
				"status": {
					"$id": "#/properties/data/properties/status",
					"title": "Status code",
					"description": "Status code.",
					"type": "integer"
				},
				"statusStr": {
					"$id": "#/properties/data/properties/statusStr",
					"title": "Status string",
					"description": "Verbose status string",
					"type": "string"
				}
			}
		}
	}
}
