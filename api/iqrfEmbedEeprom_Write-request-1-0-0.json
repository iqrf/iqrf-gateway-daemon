{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Embed EEPROM - Write",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "Embedded peripheral EEPROM - Write request.",
			"const": "iqrfEmbedEeprom_Write"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"req"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"req": {
					"$id": "#/properties/data/properties/req",
					"title": "Request data",
					"description": "Request body object",
					"type": "object",
					"required": [
						"nAdr",
						"param"
					],
					"additionalProperties": false,
					"properties": {
						"nAdr": {"$ref": "file://definitions.json#/general/nAdr"},
						"hwpId": {"$ref": "file://definitions.json#/general/hwpId"},
						"param": {
							"$id": "#/properties/data/properties/req/properties/param",
							"title": "Request parameters",
							"description": "Request message configuration parameters.",
							"type": "object",
							"required": [
								"address",
								"pData"
							],
							"additionalProperties": false,
							"properties": {
								"address": {
									"$id": "#/properties/data/properties/req/properties/param/properties/address",
									"title": "EEPROM address",
									"description": "EEPROM address to start writing data to.",
									"type": "integer"
								},
								"pData": {
									"$id": "#/properties/data/properties/req/properties/param/properties/pData",
									"title": "Data array",
									"description": "Data to be written.",
									"type": "array",
									"minItems": 1,
									"maxItems": 57,
									"additionalItems": false,
									"items": {
										"$ref": "file://definitions.json#/embed/memories/data"
									}
								}
							}
						}
					},
					"if": {
						"properties": {
							"nAdr": {"const": 0}
						}
					},
					"then": {
						"properties": {
							"param": {
								"properties": {
									"address": {
										"minimum": 0,
										"maximum": 63
									}
								}
							}
						}
					},
					"else": {
						"properties": {
							"param": {
								"properties": {
									"address": {
										"minimum": 0,
										"maximum": 191
									}
								}
							}
						}
					}
				},
				"timeout": {"$ref": "file://definitions.json#/general/timeout"},
				"returnVerbose": {"$ref": "file://definitions.json#/general/request/returnVerbose"}
			}
		}
	}
}
