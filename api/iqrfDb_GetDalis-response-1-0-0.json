{
	"$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
	"title": "GetDalis response",
	"description": "Retrieves addresses of devices implementing the DALI standard.",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$id": "#/properties/mType",
			"title": "Message type",
			"description": "IQRF DB - GetDalis response.",
			"type": "string",
			"enum": [
				"iqrfDb_GetDalis"
			]
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Response data",
			"description": "Response data.",
			"type": "object",
			"required": [
				"msgId",
				"rsp"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {
					"$id": "#/properties/data/properties/msgId",
					"title": "Message ID",
					"description": "Message ID used for pairing requests and responses.",
					"type": "string"
				},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response parameters",
					"description": "Response parameters.",
					"type": "object",
					"required": [
						"daliDevices"
					],
					"additionalProperties": false,
                    "properties": {
                        "daliDevices": {
                            "$id": "#/properties/data/properties/rsp/properties/daliDevices",
                            "title": "DALI devices",
                            "description": "Array containing addresses of devices implementing DALI standard.",
                            "type": "array",
                            "additionalItems": false,
                            "items": {
                                "$id": "#/properties/data/properties/rsp/properties/daliDevices/items",
                                "title": "Device address",
                                "description": "Address of device implementing DALI standard.",
                                "type": "integer",
                                "minimum": 0,
                                "maximum": 239
                            }
                        }
                    }
				},
				"insId": {
					"$id": "#/properties/data/properties/insId",
					"title": "Instance ID",
					"description": "Daemon insteance ID.",
					"type": "string"
				},
				"status": {
					"$id": "#/properties/data/properties/status",
					"title": "Status code",
					"description": "Status code.",
					"type": "integer"
				},
				"statusStr": {
					"$id": "#/properties/data/properties/statusStr",
					"title": "Status string",
					"description": "Verbose status string",
					"type": "string"
				}
			}
		}
	}
}
