{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Embed Node - Read",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "Embedded peripheral Node - Read response.",
			"const": "iqrfEmbedNode_Read"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"rsp",
				"status"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response data",
					"description": "Response body object",
					"type": "object",
					"required": [
						"nAdr",
						"hwpId",
						"rCode",
						"dpaVal"
					],
					"additionalProperties": false,
					"properties": {
						"nAdr": {"$ref": "file://definitions.json#/embed/node/nAdr"},
						"hwpId": {"$ref": "file://definitions.json#/general/response/hwpId"},
						"rCode": {"$ref": "file://definitions.json#/general/response/rCode"},
						"dpaVal": {"$ref": "file://definitions.json#/general/response/dpaVal"},
						"result": {
							"$id": "#/properties/data/properties/rsp/properties/result",
							"title": "Result object",
							"description": "Request result data.",
							"type": "object",
							"required": [
								"ntwADDR",
								"ntwVRN",
								"ntwZIN",
								"ntwDID",
								"ntwPVRN",
								"ntwUSERADDRESS",
								"ntwID",
								"ntwVRNFNZ",
								"ntwCFG",
								"flags"
							],
							"additionalProperties": false,
							"properties": {
								"ntwADDR": {
									"type": "integer",
									"description": "Network address."
								},
								"ntwVRN": {
									"type": "integer",
									"description": "Virtual routing number."
								},
								"ntwZIN": {
									"type": "integer",
									"description": "Zone index."
								},
								"ntwDID": {
									"type": "integer",
									"description": "Discovery id."
								},
								"ntwPVRN": {
									"type": "integer",
									"description": "Parent virtual routing number."
								},
								"ntwUSERADDRESS": {
									"type": "integer",
									"description": "User address 2B."
								},
								"ntwID": {
									"type": "integer",
									"description": "Network identification (NID0/NID1)."
								},
								"ntwVRNFNZ": {
									"type": "integer",
									"description": "VRN of first Node in given zone."
								},
								"ntwCFG": {
									"type": "integer",
									"description": "Network configuration."
								},
								"flags": {
									"type": "integer",
									"description": "Indicates whether the Node device is bonded."
								}
							}
						},
						"errorStr": {"$ref": "file://definitions.json#/general/response/errorStr"}
					}
				},
				"raw": {"$ref": "file://definitions.json#/general/response/raw"},
				"insId": {"$ref": "file://definitions.json#/general/response/insId"},
				"status": {"$ref": "file://definitions.json#/general/response/status"},
				"statusStr": {"$ref": "file://definitions.json#/general/response/statusStr"},
				"timeout": {"$ref": "file://definitions.json#/general/timeout"}
			}
		}
	}
}
