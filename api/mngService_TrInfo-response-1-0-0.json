{
	"$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
	"self": {
		"vendor": "com.iqrftech.self-desc",
		"name": "mngService_TrInfo-response",
		"format": "jsonschema",
		"version": "1-0-0"
	},
	"title": "Daemon management - Transceiver information",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$id": "#/properties/mType",
			"title": "Message type",
			"description": "Daemon management - Transceiver information response.",
			"type": "string",
			"enum": ["mngService_TrInfo"]
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body.",
			"type": "object",
			"required": [
				"msgId"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {
					"$id": "#/properties/data/properties/msgId",
					"title": "Message ID",
					"description": "Message ID used for pairing requests and responses.",
					"type": "string"
				},
        "rsp": {
          "$id": "#/properties/data/properties/rsp",
          "title": "Response data",
          "description": "Object containing command response data.",
          "type": "object",
          "required": [
            "mid",
            "osBuild",
            "osVersion",
            "trMcuType"
          ],
          "additionalProperties": false,
          "properties": {
            "mid": {
              "$id": "#/properties/data/properties/mid",
              "title": "MID",
              "description": "Module ID of Transceiver.",
              "type": "integer",
              "minimum": 0,
              "maximum": 4294967295
            },
            "osBuild": {
              "$id": "#/properties/data/properties/rsp/osBuild",
              "title": "OS build",
              "description": "OS build number.",
              "type": "integer",
              "minimum": 0,
              "maximum": 65535
            },
            "osVersion": {
              "$id": "#/properties/data/properties/rsp/osVersion",
              "title": "OS version",
              "description": "OS version number.",
              "type": "integer",
              "minimum": 0,
              "maximum": 255
            },
            "trMcuType": {
              "$id": "#/properties/data/properties/rsp/trMcuType",
              "title": "TR-MCU type",
              "description": "Transceiver and MCU identifier.",
              "type": "integer",
              "minimum": 0,
              "maximum": 255
            }
          }
        },
				"insId": {
					"$id": "#/properties/data/properties/insId",
					"title": "Instance ID",
					"description": "Daemon insteance ID.",
					"type": "string"
				},
				"status": {
					"$id": "#/properties/data/properties/status",
					"title": "Status code",
					"description": "Status code.",
					"type": "integer"
				},
				"statusStr": {
					"$id": "#/properties/data/properties/statusStr",
					"title": "Status string",
					"description": "Verbose status string",
					"type": "string"
				}
			}
		}
	}
}
