{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "IQRF Sensor Data - Get configuration",
  "description": "(c) IQRF Tech",
  "type": "object",
  "required": [
    "mType",
    "data"
  ],
  "additionalProperties": false,
  "properties": {
    "mType": {
      "allOf": [
        { "$ref": "definitions.json#/message/type" },
        { "const": "iqrfSensorData_GetConfig" }
      ]
    },
    "data": {
      "title": "Message data",
      "description": "Contains the message payload..",
      "type": "object",
      "required": [
        "msgId",
        "status"
      ],
      "additionalProperties": false,
      "properties": {
        "msgId": {
          "$ref": "definitions.json#/message/id"
        },
        "rsp": {
          "title": "Response object",
          "description": "Response object.",
          "type": "object",
          "required": [
            "autoRun",
            "period",
            "retryPeriod",
            "asyncReports",
            "messagingList"
          ],
          "additionalProperties": false,
          "properties": {
            "autoRun": {
              "$ref": "definitions.json#/sensorData/autoRun"
            },
            "period": {
              "$ref": "definitions.json#/sensorData/period"
            },
            "retryPeriod": {
              "$ref": "definitions.json#/sensorData/retryPeriod"
            },
            "asyncReports": {
              "$ref": "definitions.json#/sensorData/asyncReports"
            },
            "messagingList": {
              "$ref": "definitions.json#/sensorData/messagingList"
            }
          }
        },
        "insId": {
          "$ref": "definitions.json#/response/insId"
        },
        "status": {
          "$ref": "definitions.json#/response/status"
        },
        "statusStr": {
          "$ref": "definitions.json#/response/statusStr"
        }
      }
    }
  }
}
