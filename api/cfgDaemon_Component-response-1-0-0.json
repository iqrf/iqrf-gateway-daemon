{
    "$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.iqrftech.self-desc",
        "name": "cfgDaemon_Component-response",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "title": "Component configuration.",
    "description": "Component configuration update response.",
    "type": "object",
    "required": [
        "mType",
        "data"
    ],
    "additionalProperties": false,
    "properties": {
        "mType": {
            "$id": "#/properties/mType",
            "title": "Message type",
            "type": "string",
            "enum": [
                "cfgDaemon_Component"
            ]
        },
        "data": {
            "$id": "#/properties/data",
            "title": "Response data",
            "type": "object",
            "required": [
                "msgId",
                "rsp",
                "status"
            ],
            "additionalProperties": false,
            "properties": {
                "msgId": {
                    "$id": "#/properties/data/properties/msgId",
                    "title": "Message ID",
                    "description": "Message identification for binding request with response.",
                    "type": "string"
                },
                "rsp": {
                    "$id": "#/properties/data/properties/rsp",
                    "title": "Response object",
                    "type": "object",
                    "required": [
                        "componentName",
                        "componentInstance"
                    ],
                    "additionalProperties": false,
                    "properties": {
                        "componentName": {
                            "$id": "#/properties/data/properties/rsp/properties/componentName",
                            "description": "Component name.",
                            "type": "string"                            
                        },   
                        "componentInstance": {
                            "$id": "#/properties/data/properties/rsp/properties/componentInstance",
                            "description": "Component instance.",
                            "type": "string"                            
                        }
                    }
                },
                "insId": {
                    "$id": "#/properties/data/properties/insId",
                    "description": "IQRF GW daemon instance identification.",
                    "type": "string"
                },
                "status": {
                    "$id": "#/properties/data/properties/status",
                    "description": "Status of operation.",
                    "type": "integer"
                },
                "statusStr": {
                    "$id": "#/properties/data/properties/statusStr",
                    "description": "IQRF GW daemon API (general or mType) status in string form.",
                    "type": "string"                    
                },
                "errorStr": {
                    "$id": "#/properties/data/properties/errorStr",
                    "description": "reason of error in string form.",
                    "type": "string"
                }
            }
        }
    }
}
