{
	"$schema": "https://apidocs.iqrf.org/iqrf-gateway-daemon/latest/com.iqrftech.self-desc/schema/jsonschema/1-0-0#",
	"self": {
		"vendor": "com.iqrftech.self-desc",
		"name": "iqrfLight_SetPower-response",
		"format": "jsonschema",
		"version": "1-0-0"
	},
	"title": "IQRF Light - Set Power",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$id": "#/properties/mType",
			"title": "Message type",
			"description": "Standard peripheral Light - Set Power response.",
			"enum": [
				"iqrfLight_SetPower"
			]
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"rsp",
				"status"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {
					"$id": "#/properties/data/properties/msgId",
					"title": "Message ID",
					"description": "Message identification for binding request with response.",
					"type": "string"
				},
				"rsp": {
					"$id": "#/properties/data/properties/rsp",
					"title": "Response data",
					"description": "Response body object",
					"type": "object",
					"required": [
						"nAdr",
						"hwpId",
						"rCode",
						"dpaVal"
					],
					"additionalProperties": false,
					"properties": {
						"nAdr": {
							"$id": "#/properties/data/properties/rsp/properties/nAdr",
							"title": "Node address",
							"description": "Network device address.",
							"type": "integer",
							"minimum": 0,
							"maximum": 239
						},
						"hwpId": {
							"$id": "#/properties/data/properties/rsp/properties/hwpid",
							"title": "Device HWPID",
							"description": "Hardware profile identification.",
							"type": "integer",
							"minimum": -1,
							"maximum": 65535,
							"default": 65535
						},
						"rCode": {
							"$id": "#/properties/data/properties/rsp/properties/rCode",
							"title": "Return code",
							"description": "DPA error code.",
							"type": "integer",
							"minimum": 0
						},
						"dpaVal": {
							"$id": "#/properties/data/properties/rsp/properties/dpaVal",
							"title": "DPA value",
							"description": "DPA value.",
							"type": "integer"
						},
						"result": {
							"$id": "#/properties/data/properties/rsp/properties/result",
							"title": "Result object",
							"description": "Request result data.",
							"type": "object",
							"required": [
								"prevVals"
							],
							"additionalProperties": false,
							"properties": {
								"prevVals": {
									"$id": "#/properties/data/properties/rsp/properties/result/properties/prevVals",
									"title": "Previous power",
									"description": "Array integers corresponding to the previous power level of each selected light. If the unimplemented light was selected the returned value is 0.",
									"type": "array",
									"additionalItems": false,
									"items": {
										"$id": "#/properties/data/properties/rsp/properties/result/properties/prevVals/items",
										"title": "Light power",
										"description": "Power of a selected light",
										"type": "integer",
										"minimum": 0,
										"maximum": 100
									}
								}
							}
						},
						"errorStr": {
							"$id": "#/properties/data/properties/rsp/properties/errorStr",
							"title": "Error string",
							"description": "Error description from JS drivers.",
							"type": "string"							
						}
					}
				},
				"raw": {
					"$id": "#/properties/data/properties/raw",
					"title": "Raw DPA messages",
					"description": "Returns array of objects req-cnf-rsp, see its description.",
					"type": "array",
					"additionalItems": false,
					"items": {
						"$id": "#/properties/data/properties/raw/items",
						"title": "Raw DPA messages object",
						"description": "Object containing Request, Confirmation and Response DPA messages and their timestamps.",
						"type": "object",
						"required": [
							"request",
							"requestTs",
							"confirmation",
							"confirmationTs",
							"response",
							"responseTs"
						],
						"additionalProperties": false,
						"properties": {
							"request": {
								"$id": "#/properties/data/properties/raw/items/properties/request",
								"title": "Request DPA",
								"description": "Binary buffer with DPA request.",
								"type": "string"
							},
							"requestTs": {
								"$id": "#/properties/data/properties/raw/items/properties/requestTs",
								"title": "Request timestamp",
								"description": "YYYY-MM-DDTHH:MM:SS.SSS±HH:MM",
								"type": "string"
							},
							"confirmation": {
								"$id": "#/properties/data/properties/raw/items/properties/confirmation",
								"title": "Confirmation DPA",
								"description": "Binary buffer with DPA confirmation.",
								"type": "string"
							},
							"confirmationTs": {
								"$id": "#/properties/data/properties/raw/items/properties/confirmationTs",
								"title": "Confirmation timestamp",
								"description": "YYYY-MM-DDTHH:MM:SS.SSS±HH:MM",
								"type": "string"
							},
							"response": {
								"$id": "#/properties/data/properties/raw/items/properties/response",
								"title": "Request DPA",
								"description": "Binary buffer with DPA response.",
								"type": "string"
							},
							"responseTs": {
								"$id": "#/properties/data/properties/raw/items/properties/responseTs",
								"title": "Response DPA",
								"description": "YYYY-MM-DDTHH:MM:SS.SSS±HH:MM",
								"type": "string"
							}
						}
					}
				},
				"insId": {
					"$id": "#/properties/data/properties/insId",
					"title": "Instance ID",
					"description": "IQRF GW daemon instance identification.",
					"type": "string"
				},
				"status": {
					"$id": "#/properties/data/properties/status",
					"title": "Status code",
					"description": "Status of operation.",
					"type": "integer"
				},
				"statusStr": {
					"$id": "#/properties/data/properties/statusStr",
					"title": "Status string",
					"description": "IQRF GW daemon API (general or mType) status in string form.",
					"type": "string"
				},
				"timeout": {
					"$id": "#/properties/data/properties/timeout",
					"title": "Request timeout",
					"description": "Timeout to wait for IQRF DPA response.",
					"type": "integer"
				}
			}
		}
	}
}
