{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Daemon managerment - Mode",
	"type": "object",
	"required": [
		"mType",
		"data"
	],
	"additionalProperties": false,
	"properties": {
		"mType": {
			"$ref": "file://definitions.json#/general/mType",
			"description": "Daemon runtime management - Change the daemon mode response.",
			"const": "mngDaemon_Mode"
		},
		"data": {
			"$id": "#/properties/data",
			"title": "Message data",
			"description": "Message metadata and body",
			"type": "object",
			"required": [
				"msgId",
				"rsp",
				"status"
			],
			"additionalProperties": false,
			"properties": {
				"msgId": {"$ref": "file://definitions.json#/general/msgId"},
				"rsp": {
					"$id": "#/properties/data",
					"title": "Message data",
					"description": "Message metadata and body",
					"type": "object",
					"required": [
						"operMode"
					],
					"additionalProperties": false,
					"properties": {
						"operMode": {
							"$id": "#/properties/data/properties/rsp/properties/operMode",
							"title": "Operation mode",
							"description": "Operation mode.",
							"type": "string",
							"enum": [
								"operational",
								"service",
								"forwarding",
								"unknown"
							]
						}
					}
				},
				"insId": {"$ref": "file://definitions.json#/general/response/insId"},
				"status": {"$ref": "file://definitions.json#/general/response/status"},
				"statusStr": {"$ref": "file://definitions.json#/general/response/statusStr"}
			}
		}
	}
}
